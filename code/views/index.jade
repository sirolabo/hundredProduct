        ="更新情報ああああああああああああああああああああああああああああああああああああああああああ"
    img#story(src=path.img + "imageList/h_story.jpg" data-webele="headImage")
    img(src=path.img + "imageList/story.jpg" data-webele="mainImage")
    img#char(src=path.img + "imageList/h_char.jpg" data-webele="headImage")
    img(src=path.img + "imageList/char_A.jpg" data-webele="mainImage")
    img(src=path.img + "imageList/char_B.jpg" data-webele="mainImage")
    img(src=path.img + "imageList/h_gallery.jpg" data-webele="headImage")
