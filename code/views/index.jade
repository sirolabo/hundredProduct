extends _core
block head
  +makeHead("Home")
block header
  #header2(data-webele="header")
    a(href="#" data-webele="MenuButton")
      ="HOME"
    a(href="#story" data-webele="MenuButton")
      ="STORY"
    a(href="#char" data-webele="MenuButton")
      ="CHARACTER"
    a(href="#gallery" data-webele="MenuButton")
      ="GALLERY"
    a(href="#dl" data-webele="MenuButton")
      ="DOWNLOAD"
block main
  .frontBG(data-webele="frontBG")
